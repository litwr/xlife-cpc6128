#fix 8L-files
gcc -x c - <<END
#include<stdio.h>
main(){
   int i, k;
   char b[128];
   fread(b, 1, 128, stdin);
   putchar(i = getchar());
   putchar(k = getchar());
   i = (k*256 + i + 3)*2;
   for (k = 0; k < i; k++) putchar(getchar());
}
END
for i in *.8L
do
   ./a.out <$i >/tmp/$i
   mv /tmp/$i $i
done
rm a.out
